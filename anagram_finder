import string
import itertools


def generate_two_letter_combinations():
    # Use string.ascii_uppercase for 'AA', 'AB'...
    # Use string.ascii_lowercase for 'aa', 'ab'...
    alphabet = string.ascii_uppercase

    # Generate the product of the alphabet with a repeat of 2
    combinations = itertools.product(alphabet, repeat=2)

    # Join the tuples into strings and print
    for combo in combinations:
        print(''.join(combo))


if __name__ == "__main__":
    generate_two_letter_combinations()
